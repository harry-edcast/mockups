	background:transparent url(/cesimages/CesImageStream.ashx?lm=defaultTabs&color=ff004f71&enc=1&nogradiant=False&uselineargradient=False&path=%2fDynamicContent%2fimages%2fdialog%2fpanelRound-title.png&browser=SF) no-repeat scroll left top;
}
#dir-rtl .widgetcntnr #toppnlleft { background-position: right top; }
.widgetcntnr #toppnlright
{
	background:transparent url(/cesimages/CesImageStream.ashx?lm=defaultTabs&color=ff004f71&enc=1&nogradiant=False&uselineargradient=False&path=%2fDynamicContent%2fimages%2fdialog%2fpanelRound-title.png&browser=SF) no-repeat scroll right top;
}
#dir-rtl .widgetcntnr #toppnlright { background-position: left top; }
@media screen {
	.widgetcntnr #pnlcontent
	{
		border-left:solid 1px #DDDDDD;
		border-right:solid 1px #DDDDDD;
		border-bottom:solid 1px #DDDDDD;
	}
}
.widgetcntnr #pnlcontent
{
	overflow:auto;
	zoom:1;
}
