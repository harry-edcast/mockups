	background:transparent url(/cesimages/CesImageStream.ashx?lm=defaultTabs&color=ff004f71&enc=1&nogradiant=False&uselineargradient=False&path=%2fDynamicContent%2fimages%2fdialog%2fpanelRound-title.png&browser=SF) repeat-x scroll left bottom;
	background-position:0px -27px;
	color:#FFFFFF;
	font-weight:bold;
	font-size:11px;
	height:25px;
	line-height:25px;
	white-space:nowrap;
	vertical-align:middle;
}
